public static int[] generierePin() {
	int[] pin = new int[4];
	
	for (int i=0; i<pin.length; i++) {
		pin[i] = (int) (Math.random()*10);
	for(int j = i+1; j<pin.length; j++) {
		while(i == j)
			pin[i] = (int) (Math.random()*10);
		
		
	}

	System.out.println("Pincode i: " + pin[i]);
	
	}


		
		
	return pin;	
	}

		



